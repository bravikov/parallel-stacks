{
  "name": "parallel-stacks",
  "displayName": "Parallel Stacks",
  "description": "The Parallel Stacks extension is useful for debugging multithreaded applications.",
  "version": "0.0.3",
  "publisher": "bravikov",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Debuggers"
  ],
  "icon": "images/icon.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "parallel-stacks.show",
        "title": "Parallel Stacks"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "@viz-js/viz": "^3.16.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@types/vscode": "^1.99.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bravikov/parallel-stacks"
  }
}
